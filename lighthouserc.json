{
  "ci": {
    "collect": {
      "startServerCommand": "npx http-server dist -p 8080 -s",
      "url": [
        "http://localhost:8080/"
      ],
      "numberOfRuns": 2
    },
    "assert": {
      "assertions": {
        "categories:performance": [
          "warn",
          {
            "minScore": 0.9
          }
        ],
        "categories:accessibility": [
          "error",
          {
            "minScore": 0.95
          }
        ],
        "categories:best-practices": [
          "error",
          {
            "minScore": 0.9
          }
        ],
        "categories:seo": [
          "error",
          {
            "minScore": 0.95
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}